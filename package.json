{
  "name": "cytoscape-angular-workspace",
  "version": "1.0.0",
  "description": "Angular workspace for cytoscape-angular library and demo",
  "scripts": {
    "ng": "ng",
    "start": "ng serve demo",
    "build": "ng build cytoscape-angular",
    "build:prod": "ng build cytoscape-angular --configuration production",
    "build:demo": "ng build demo --configuration production",
    "build:demo:dev": "ng build demo",
    "watch": "ng build cytoscape-angular --watch --configuration development",
    "test": "ng test",
    "test:lib": "ng test cytoscape-angular",
    "test:demo": "ng test demo",
    "test:ci": "ng test --watch=false --browsers=ChromeHeadless --code-coverage",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.0",
    "@angular/cdk": "^20.0.0",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/material": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-browser-dynamic": "^20.0.0",
    "@angular/router": "^20.0.0",
    "cytoscape": "^3.30.2",
    "cytoscape-dagre": "^2.5.0",
    "rxjs": "^7.8.1",
    "tslib": "^2.7.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.0",
    "@angular-eslint/builder": "^20.0.0",
    "@angular-eslint/eslint-plugin": "^20.0.0",
    "@angular-eslint/eslint-plugin-template": "^20.0.0",
    "@angular-eslint/schematics": "^20.0.0",
    "@angular-eslint/template-parser": "^20.0.0",
    "@angular/cli": "^20.0.0",
    "@angular/compiler-cli": "^20.0.0",
    "@types/cytoscape": "^3.21.8",
    "@types/jasmine": "~5.1.4",
    "@types/node": "^22.7.5",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "eslint": "^9.12.0",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.0.0",
    "typescript": "~5.8.2"
  }
}
